<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <IsPackable>true</IsPackable>
    <Authors>hack3rlife</Authors>
    <Description>A simple CSharp HttpClient Wrapper</Description>
    <PackageProjectUrl>https://github.com/hack3rlife/asp-net-core-rest-client</PackageProjectUrl>
    <RepositoryUrl>https://github.com/hack3rlife/asp-net-core-rest-client</RepositoryUrl>
    <PackageTags>dotnet; c#; httpclient; hack3rlife</PackageTags>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

</Project>
